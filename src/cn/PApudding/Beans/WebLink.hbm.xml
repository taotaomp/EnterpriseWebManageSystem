<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<!-- 建立WebLink类与WebLink表关系映射  -->
	<class name="cn.PApudding.Beans.WebLink" table="WebLink">
		<!-- 主键映射 -->
		<id name="id" column="id">
			<!-- native生成策略 -->
			<generator class="native"/>
		</id>
		
		<!-- 其他字段映射 -->
		<property name="linkName" column="linkName"/>
		<property name="linkUrl" column="linkUrl"/>
		<property name="linkField" column="linkField" not-null="true"/>
	</class>
</hibernate-mapping>