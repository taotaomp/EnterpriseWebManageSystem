<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<!-- 建立WebPage类与WebPage表关系映射  -->
	<class name="cn.PApudding.Beans.WebPage" table="WebPage">
		<!-- 主键映射 -->
		<id name="id" column="id">
			<!-- native生成策略 -->
			<generator class="native"/>
		</id>
		
		<!-- 其他字段映射 -->
		<property name="pageName" column="pageName" unique="true"/>
		<property name="pageFileCode" column="pageFileCode" type="text"/>
		<!-- 集合的映射 -->
		<set name="pageModelNames" table="WebPage_pageModelNames">
			<key column="id"></key>
			<element type="string" column="ModelNames"></element>
		</set>
	</class>
</hibernate-mapping>